<div class="card-body">
    <div class="card-wrapper">
        <% cardData.forEach((card)=>{ %>
         <div class="card">
            <figure class="card-image">
                <img width="320" height="200" src="../img/<%= card.image %>" alt="<%= card.alt %>">
            </figure>
            <div class="card-text-body">
                <p class="card-text">
                    <%= card.description %>
                </p>
            </div>
         </div>
        <% }) %>
    </div>
</div>